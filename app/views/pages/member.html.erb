<div class='panel'>
    <div class='panel-heading'>
        <h4>Profile</h4>
        <div class="panel-body">
                
            <div class="row mb10">
                <div class='col-xs-6 col-md-4 col-lg-2'>
                    <h5>First Name:</h5>
                </div>
                <div class='col-xs-6 col-md-4 col-lg-3'>
                    <h5><%= current_user.first_name %></h5>
                </div>
            </div>
            <br>
             <div class="row mb10">
                <div class='col-xs-6 col-md-4 col-lg-2'>
                    <h5>Last Name:</h5>
                </div>
                <div class='col-xs-6 col-md-4 col-lg-3'>
                    <h5><%= current_user.last_name %></h5>
                </div>
            </div>
            <br>
             <div class="row mb10">
                <div class='col-xs-6 col-md-4 col-lg-2'>
                    <h5>Email:</h5>
                </div>
                <div class='col-xs-6 col-md-4 col-lg-3'>
                    <h5><%= current_user.email %></h5>
                </div>
            </div>
            <br>
            
        </div>
    </div>
</div>